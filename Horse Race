controller.player1.onEvent(ControllerEvent.Connected, function () {
    player1 = sprites.create(img`
        ..............2..2......
        .............e2e222.....
        .............e222f2222..
        ...........eee22222222..
        ............ee2222222...
        ..........eeee2222......
        ..ee.2222222222222......
        .eeee2222222222222......
        ..ee.2222222222222......
        e.e..2222222222222eee...
        .e...222222222222...e...
        .....22ee.....2222.f....
        .....2.ee........2......
        ....2..e........f.......
        ....2.f.................
        ...f....................
        `, SpriteKind.Player)
    player1.setPosition(5, 25)
    controller.player1.moveSprite(player1, 20, 5)
})

controller.player2.onEvent(ControllerEvent.Connected, function () {
    player2 = sprites.create(img`
        ..............8..8......
        .............989888.....
        .............9888f8888..
        ...........99988888888..
        ............998888888...
        ..........99998888......
        ..99.8888888888888......
        .99998888888888888......
        ..99.8888888888888......
        9.9..8888888888888999...
        .9...888888888888...9...
        .....8899.....8888.f....
        .....8.99........8......
        ....8..9........f.......
        ....8.f.................
        ...f....................
        `, SpriteKind.Player)
    player2.setPosition(5, 50)
    controller.player2.moveSprite(player2, 20, 5)
})

controller.player3.onEvent(ControllerEvent.Connected, function () {
    player3 = sprites.create(img`
        ..............5..5......
        .............454555.....
        .............4555f5555..
        ...........44455555555..
        ............445555555...
        ..........44445555......
        ..44.5555555555555......
        .44445555555555555......
        ..44.5555555555555......
        4.4..5555555555555444...
        .4...555555555555...4...
        .....5544.....5555.f....
        .....5.44........5......
        ....5..4........f.......
        ....5.f.................
        ...f....................
        `, SpriteKind.Player)
    player3.setPosition(5, 75)
    controller.player3.moveSprite(player3, 20, 5)
})

controller.player4.onEvent(ControllerEvent.Connected, function () {
    player4 = sprites.create(img`
        ..............7..7......
        .............676777.....
        .............6777f7777..
        ...........66677777777..
        ............667777777...
        ..........66667777......
        ..66.7777777777777......
        .66667777777777777......
        ..66.7777777777777......
        6.6..7777777777777666...
        .6...777777777777...6...
        .....7766.....7777.f....
        .....7.66........7......
        ....7..6........f.......
        ....7.f.................
        ...f....................
        `, SpriteKind.Player)
    player4.setPosition(5, 100)
    controller.player4.moveSprite(player4, 20, 5)
})

sprites.onOverlap(SpriteKind.Player, SpriteKind.finish, function (sprite, otherSprite) {
    if (player1.overlapsWith(finishline)) {
        player1.sayText("I WON")
        player1.startEffect(effects.confetti, 100)
    } else if (player2.overlapsWith(finishline)) {
        player2.sayText("I WON")
        player2.startEffect(effects.confetti, 100)
    } else if (player3.overlapsWith(finishline)) {
        player3.sayText("I WON")
        player3.startEffect(effects.confetti, 100)
    } else if (player4.overlapsWith(finishline)) {
        player4.sayText("I WON")
        player4.startEffect(effects.confetti, 100)
    }
})
```
